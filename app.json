{
    "name": "Uptodate",
    "description": "Tech News Broadcaster",
    "repository": "https://github.com/TinuraD/UptoDate-Bot",
    "logo": "https://user-images.githubusercontent.com/87059430/145522135-655760bd-c4aa-4cee-b7c7-ca4750b7bdd3.jpg",
    "keywords": ["uptodate", "tech news"],
    "env": {
      "BOT_TOKEN": {
        "description": "Get your Bot token from @Botfather",
        "required": true,
        "value": ""
      },
      "DATABASE_URL": {
        "description": "If heroku, no need to fill this.",
        "required": false,
        "value": ""
      },
      "SUDO_ID": {
        "description": "SUDO IDs for the bot.",
        "required": true,  
        "value": ""
      }
     },
     "addons": [
        {
           "options": {
              "version": "12"
           },
           "plan": "heroku-postgresql"
        }
     ],
    "buildpacks": [
        {
          "url": "heroku/python"
        }
      ]
  }
  
